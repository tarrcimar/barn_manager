+package example.controller;
+
+import com.jfoenix.controls.JFXListView;
+import javafx.beans.Observable;
+import javafx.collections.FXCollections;
+import javafx.collections.ObservableList;
+import javafx.collections.ObservableMap;
+import javafx.fxml.FXML;
+import javafx.scene.control.Hyperlink;
+import javafx.scene.layout.AnchorPane;
+
+public class ListController {
+
+    @FXML
+    private AnchorPane rootPane;
+
+    @FXML
+    private Hyperlink backToMain;
+
+    @FXML
+    private JFXListView<String> barnList;
+    ObservableList<String> listanezet = FXCollections.observableArrayList("Csillag",
+            "Villám","Ráró","Sebes","Bokréta","Kincsem","Napszállta","Bosszúálló","Kapitány","Sanyi");
+
+    @FXML
+    void initialize(){
+            barnList.setItems(listanezet);
+        backToMain.setOnAction(actionEvent -> new FadeController().fadeOut("/example/view/mainWindow.fxml", rootPane));
+    }
+}
